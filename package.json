{
  "name": "recoil-spring",
  "version": "0.1.7",
  "description": "Jumpstart Recoil - less boilerplate, more fun.",
  "main": "index.cjs.js",
  "module": "index.es.js",
  "types": "lib/ts/index.d.ts",
  "repository": "https://github.com/yoavniran/recoil-spring",
  "author": "yoav niran",
  "keywords": [
    "recoil",
    "state",
    "state management",
    "react",
    "hooks"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rimraf lib",
    "build": "yarn clean; rollup -c --bundleConfigAsCjs",
    "watch": "yarn build --watch",
    "lint": "eslint ./src --cache",
    "lint:nocache": "eslint ./src",
    "mocha": "NODE_ENV=test mocha --parallel",
    "ts:test": "dtslint types; tsc --project tsconfig.json --noEmit",
    "ts:gen": "node scripts/defgen.js --in types/index.d.ts --out lib/ts/index.d.ts",
    "test": "yarn lint; yarn mocha",
    "release": "release-it --verbose",
    "release:same": "yarn release --no-increment",
    "release:rc": "yarn release --preRelease=rc",
    "cy:open": "CYPRESS_BASE_URL=http://localhost:8050 cypress open --e2e --browser chrome",
    "cy:comp": " cypress open --component --browser chrome"
  },
  "peerDependencies": {
    "recoil": ">=0.7.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@definitelytyped/dtslint": "^0.0.142",
    "@istanbuljs/nyc-config-babel": "^3.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.2.0",
    "@testing-library/cypress": "^8.0.7",
    "@testing-library/react": "^13.4.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.3.0",
    "babel-plugin-istanbul": "^6.1.1",
    "chai": "^4.3.7",
    "concurrently": "^7.6.0",
    "cypress": "^12.2.0",
    "dts-generator": "^3.0.0",
    "eslint": "^8.29.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-no-async": "^1.0.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-storybook": "^0.6.8",
    "global-jsdom": "^8.6.0",
    "jsdom": "^20.0.3",
    "mocha": "^10.1.0",
    "mocha-junit-reporter": "^2.2.0",
    "mocha-multi-reporters": "^1.5.1",
    "nyc": "^15.1.0",
    "random-name": "^0.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recoil": "^0.7.6",
    "release-it": "^15.5.1",
    "rollup": "^3.7.4",
    "sinon": "^15.0.0",
    "typescript": "^4.9.4",
    "uuid": "^9.0.0",
    "yargs": "^17.6.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "throttle-debounce": "^5.0.0"
  }
}
